  "main": "lib/index.js",
  "author": "meteorlxy <meteor.lxy@foxmail.com>",
  "license": "MIT",
  "keywords": [
    "vuepress",
    "plugin",
    "zooming",
    "image",
    "zoom"
  ],
  "homepage": "https://github.com/vuepress/vuepress-plugin-zooming",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress/vuepress-plugin-zooming.git"
  },
  "bugs": {
